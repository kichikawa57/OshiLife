{
  "name": "oshilife",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14'",
    "ios:release": "react-native run-ios --mode Release",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "eslint": "eslint '**/*.{ts,tsx}'",
    "eslint:fix": "eslint --fix '**/*.{ts,tsx}'",
    "stylelint": "stylelint --ignore-path .gitignore \"app/**/*.+(ts|tsx)\"",
    "stylelint:fix": "yarn stylelint -- --fix",
    "format": "prettier \"**/*.+(js|ts|tsx)\"",
    "format:fix": "prettier --write \"**/*.+(js|ts|tsx)\"",
    "lints": "yarn eslint && yarn format && yarn tsc",
    "lints:fix": "yarn eslint:fix && yarn format:fix && yarn tsc",
    "eas:submit": "eas submit -p ios --latest",
    "eas:update": "eas update --channel production",
    "eas:login": "eas login",
    "eas:build": "eas build --profile production --platform ios",
    "eas:build:dev": "eas build --profile development --platform ios",
    "eas:build:local": "eas build --profile development --platform ios --local",
    "tsc": "tsc",
    "check:devices": "xcrun simctl list devices",
    "pod:install": "cd ios && pod install && cd ..",
    "prepare": "husky install",
    "supabase": "supabase",
    "link:supabase": "yarn supabase link --project-ref odzswjxeygxhhnibrusv",
    "gen:schema": "yarn supabase gen types typescript --linked > app/api/schema.ts"
  },
  "resolutions": {
    "react-devtools-core": "~4.28.0"
  },
